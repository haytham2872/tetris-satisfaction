  "scripts": {
    "start": "react-scripts start",
    "start:client": "set PORT=3000 && set REACT_APP_MODE=client && react-scripts start",
    "start:admin": "set PORT=3001 && set REACT_APP_MODE=admin && react-scripts start",
    "build": "react-scripts build",
    "build:client": "set REACT_APP_MODE=client && cross-env BUILD_PATH=./build/client react-scripts build",
    "build:admin": "set REACT_APP_MODE=admin && cross-env BUILD_PATH=./build/admin react-scripts build",
    "build:all": "npm run build:client && npm run build:admin",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },

  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
